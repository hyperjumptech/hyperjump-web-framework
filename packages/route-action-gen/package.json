{
  "name": "route-action-gen",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "dist/index.d.ts",
  "bin": {
    "route-action-gen": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    },
    "./lib/next": {
      "types": "./dist/lib/next/index.d.ts",
      "import": "./dist/lib/next/index.js"
    },
    "./lib/react": {
      "types": "./dist/lib/react/index.d.ts",
      "import": "./dist/lib/react/index.js"
    },
    "./lib/node": {
      "types": "./dist/lib/node/index.d.ts",
      "import": "./dist/lib/node/index.js"
    }
  },
  "scripts": {
    "test": "vitest --run",
    "build": "tsc"
  },
  "dependencies": {
    "glob": "^13.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@workspace/typescript-config": "workspace:*",
    "next": "^15.5.9",
    "vitest": "^2.0.5"
  }
